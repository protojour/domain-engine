pub def artist {
    rel .'ID'(rel .gen: auto)|id: { fmt '' => 'artist/' => uuid => . }
    rel .'name': string
}

pub def record {
    rel .'name': string
}

pub def instrument-id {
    fmt '' => 'instrument/' => uuid => .
}

pub def instrument {
    rel .'ID'(rel .gen: auto)|id: instrument-id
    rel .'name': string
}

pub def plays {
    rel .'how_much': string
}

rel [artist] 'plays'(rel .is: plays)::'played_by' [instrument]
