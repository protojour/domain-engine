type artist-id {
    rel '' ['artist/'] [uuid]
}
type instrument-id {
    rel '' ['instrument/'] [uuid]
}

type artist
rel artist ['name'] string
rel artist [id] artist-id

type record
rel record ['name'] string

type instrument {
    rel [id] instrument-id
    rel ['name'] string
}

type plays
rel plays ['how_much'] string

rel artist ['plays'* | 'played_by'*: plays] instrument
