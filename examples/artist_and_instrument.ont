pub type artist-id {
    rel '' ['artist/'] [uuid]
}
pub type instrument-id {
    rel '' ['instrument/'] [uuid]
}

pub type artist
rel artist ['name'] string
rel artist-id [identifies] artist

pub type record
rel record ['name'] string

pub type instrument {
    rel instrument-id [identifies]
    rel ['name'] string
}

pub type plays
rel plays ['how_much'] string

rel artist ['plays'* | 'played_by'*: plays] instrument
