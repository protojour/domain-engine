use 'geojson' as geojson

def code (
    fmt '' => text => .
)

def municipality (
    rel .'code'|id: code
    rel .'geometry': geojson.Geometry
)

/// Get a single municipality
map municipality(
    code: code,
    municipality match('code': code)
)

map municipalities(
    (),
    municipality: {..municipality match()}
)
