domain 01J2QEPK751RT4Q0RSSNAK741W ()

use 'geojson' as geojson

def code (
    fmt '' => text => .
)

def municipality (
    rel. 'code': code
    rel* 'geometry': geojson.Geometry
)

/// Get a single municipality
map municipality(
    code(code),
    @match municipality('code': code)
)

map municipalities(
    (),
    municipality {..@match municipality()}
)
