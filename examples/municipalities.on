use 'geojson' as geojson

pub def code {
    fmt '' => text => .
}

pub def municipality {
    rel .'code'|id: code
    rel .'geometry': geojson.Geometry
}

/// Get a single municipality
map municipality {
    code: code
    municipality match { 'code': code }
}

