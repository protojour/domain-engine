domain 01GQNEWZ00N7FMHACAWZ973DT7 ()

use 'atlas:/protojour/geojson/geojson' as geojson

def code (
    fmt '' => text => .
)

def municipality (
    rel. 'code': code
    rel* 'geometry': geojson.Geometry
)

/// Get a single municipality
map municipality(
    code(code),
    @match municipality('code': code)
)

map municipalities(
    (),
    municipality {..@match municipality()}
)
