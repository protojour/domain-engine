# Fact syntax

(domain d1
    (type facade)
    (prop foo)
    (has! facade foo)
)

(domain d2
    (type internal)
    (prop bar)
    (has! internal bar)
)

(eq! (x)
    (d1.facade
        (foo x)
    ),
    (d2.internal
        (bar x)
    )
)

## keyword-less:

d1::{
    foo = text
    facade {
        foo
    }
}

d2::{
    bar = text
    internal {
        bar
    }
}

eq<x>(
    d1::facade {
        foo: x
    }
    d2::internal {
        bar: x
    }
)
