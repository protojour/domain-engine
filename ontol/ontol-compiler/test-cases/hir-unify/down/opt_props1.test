(struct ($b)
    (prop? $b R:1:0
        (map $a)
    )
)

(struct ($c)
    (prop? $c R:2:1
        (map $a)
    )
)

|$b| (block
    (let-prop $a ($b R:1:0))
    (struct ($c)
        (catch (@d)
            (try? @d $a)
            (prop?- $c R:2:1 (map $a))
        )
        (move-rest-attrs $c $b)
    )
)
