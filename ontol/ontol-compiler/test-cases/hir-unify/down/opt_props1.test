(struct ($b)
    (prop? $b R:1:0
        (#u (map $a))
    )
)

(struct ($c)
    (prop? $c R:2:1
        (#u (map $a))
    )
)

|$b| (block
    (let-prop $_ $a ($b R:1:0))
    (struct ($c)
        (catch (@d)
            (try? @d $a)
            (prop?- $c R:2:1
                (#u (map $a))
            )
        )
        (move-rest-attrs $c $b)
    )
)
