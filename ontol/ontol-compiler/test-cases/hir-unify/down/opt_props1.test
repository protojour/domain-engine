(struct ($b)
    (prop? $b rel@1:0:0
        (map $a)
    )
)

(struct ($c)
    (prop? $c rel@2:0:1
        (map $a)
    )
)

|$b| (block
    (let-prop $a ($b rel@1:0:0))
    (struct ($c)
        (catch (@d)
            (try? @d $a)
            (prop?- $c rel@2:0:1 (map $a))
        )
        (move-rest-attrs $c $b)
    )
)
