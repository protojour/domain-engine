// The difference with `flat_map1` is that the non-seq-prop appears _first_ here:
(struct ($d)
    (prop! $d rel@1:0:2 $a)
    (prop! $d rel@1:0:5
        (matrix
            (.. @c
                (struct ($e)
                    (prop! $e rel@1:0:4
                        [$b #u]
                    )
                )
            )
        )
    )
)

(matrix
    (.. @c
        (struct ($f)
            (prop! $f rel@1:0:7 $a)
            (prop! $f rel@1:0:8 $b)
        )
    )
)

|$d| (block
    (let-prop $a ($d rel@1:0:2))
    (let-prop [$c] ($d rel@1:0:5))
    (make-matrix ($g)
        (for-each $c ($e)
            (let-prop [$b $_] ($e rel@1:0:4))
            (insert $g
                (struct ($f)
                    (prop! $f rel@1:0:7 $a)
                    (prop! $f rel@1:0:8 $b)
                )
            )
        )
    )
)