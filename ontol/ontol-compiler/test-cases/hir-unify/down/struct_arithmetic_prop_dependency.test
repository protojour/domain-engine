(struct ($c)
    (prop! $c p@1:0:0 (- $a 10))
    (prop! $c p@1:0:1 (- $b 10))
)

(struct ($d)
    (prop! $d p@2:0:0 (+ $a $b))
    (prop! $d p@2:0:1 (+ $a 20))
    (prop! $d p@2:0:2 (+ $b 20))
)

|$c| (block
    (let-prop $e ($c p@1:0:0))
    (let-prop $f ($c p@1:0:1))
    (let $a (+ $e 10))
    (let $b (+ $f 10))
    (struct ($d)
        (prop! $d p@2:0:0 (+ $a $b))
        (prop! $d p@2:0:1 (+ $a 20))
        (prop! $d p@2:0:2 (+ $b 20))
        (move-rest-attrs $d $c)
    )
)
