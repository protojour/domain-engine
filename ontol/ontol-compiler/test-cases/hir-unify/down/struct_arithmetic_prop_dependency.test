(struct ($c)
    (prop! $c R:1:0 (- $a 10))
    (prop! $c R:1:1 (- $b 10))
)

(struct ($d)
    (prop! $d R:2:0 (+ $a $b))
    (prop! $d R:2:1 (+ $a 20))
    (prop! $d R:2:2 (+ $b 20))
)

|$c| (block
    (let-prop $e ($c R:1:0))
    (let-prop $f ($c R:1:1))
    (let $a (+ $e 10))
    (let $b (+ $f 10))
    (struct ($d)
        (prop! $d R:2:0 (+ $a $b))
        (prop! $d R:2:1 (+ $a 20))
        (prop! $d R:2:2 (+ $b 20))
        (move-rest-attrs $d $c)
    )
)
