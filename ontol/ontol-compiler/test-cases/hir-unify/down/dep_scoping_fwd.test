(struct ($c)
    (prop! $c R:1:0 (+ $b $c))
    (prop! $c R:1:1 (+ $a $b))
    (prop! $c R:1:2 $a)
)

(struct ($f)
    (prop! $f R:2:0 $a)
    (prop! $f R:2:1 $b)
    (prop! $f R:2:2 $c)
)

|$c| (block
    (let-prop $g ($c R:1:0))
    (let-prop $h ($c R:1:1))
    (let-prop $a ($c R:1:2))
    (let $b (- $a $h))
    (let $c (- $b $g))
    (struct ($f)
        (prop! $f R:2:0 $a)
        (prop! $f R:2:1 $b)
        (prop! $f R:2:2 $c)
        (move-rest-attrs $f $c)
    )
)
