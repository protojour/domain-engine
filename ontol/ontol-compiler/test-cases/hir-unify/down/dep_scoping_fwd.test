(struct ($c)
    (prop! $c p@1:0:0 (+ $b $c))
    (prop! $c p@1:0:1 (+ $a $b))
    (prop! $c p@1:0:2 $a)
)

(struct ($f)
    (prop! $f p@2:0:0 $a)
    (prop! $f p@2:0:1 $b)
    (prop! $f p@2:0:2 $c)
)

|$c| (block
    (let-prop $g ($c p@1:0:0))
    (let-prop $h ($c p@1:0:1))
    (let-prop $a ($c p@1:0:2))
    (let $b (- $a $h))
    (let $c (- $b $g))
    (struct ($f)
        (prop! $f p@2:0:0 $a)
        (prop! $f p@2:0:1 $b)
        (prop! $f p@2:0:2 $c)
        (move-rest-attrs $f $c)
    )
)
