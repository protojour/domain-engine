(struct ($c)
    (prop! $c R:1:0 (#u (+ $b $c)))
    (prop! $c R:1:1 (#u (+ $a $b)))
    (prop! $c R:1:2 (#u $a))
)

(struct ($f)
    (prop! $f R:2:0 (#u $a))
    (prop! $f R:2:1 (#u $b))
    (prop! $f R:2:2 (#u $c))
)

|$c| (block
    (let-prop $_ $g ($c R:1:0))
    (let-prop $_ $h ($c R:1:1))
    (let-prop $_ $a ($c R:1:2))
    (let $b (- $a $h))
    (let $c (- $b $g))
    (struct ($f)
        (prop! $f R:2:0
            (#u $a)
        )
        (prop! $f R:2:1
            (#u $b)
        )
        (prop! $f R:2:2
            (#u $c)
        )
        (move-rest-attrs $f $c)
    )
)
