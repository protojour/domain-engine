(struct ($b)
    (prop! $b R:1:0
        (matrix (.. @d #u $a))
    )
)

(struct ($c)
    (prop! $c R:2:1
        (matrix (.. @d #u $a))
    )
)

|$b| (block
    (let-prop [$d $e] ($b R:1:0))
    (struct ($c)
        (prop! $c R:2:1
            (make-matrix ($f $g)
                (for-each $d $e ($_ $a)
                    (insert $f #u)
                    (insert $g $a)
                )
            )
        )
        (move-rest-attrs $c $b)
    )
)
