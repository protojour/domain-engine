(struct ($b)
    (prop! $b R:1:0
        (set (.. @d #u $a))
    )
)

(struct ($c)
    (prop! $c R:2:1
        (set (.. @d #u $a))
    )
)

|$b| (block
    (let-prop $d ($b R:1:0))
    (struct ($c)
        (prop! $c R:2:1
            (make-seq ($e)
                (for-each $d ($_ $a)
                    (insert $e #u $a)
                )
            )
        )
        (move-rest-attrs $c $b)
    )
)
