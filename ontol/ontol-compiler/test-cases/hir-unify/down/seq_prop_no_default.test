(struct ($b)
    (prop! $b rel@1:0:0
        (matrix (.. @d #u $a))
    )
)

(struct ($c)
    (prop! $c rel@2:0:1
        (matrix (.. @d #u $a))
    )
)

|$b| (block
    (let-prop [$d $e] ($b rel@1:0:0))
    (struct ($c)
        (prop! $c rel@2:0:1
            (make-matrix ($f $g)
                (for-each $d $e ($_ $a)
                    (insert $f #u)
                    (insert $g $a)
                )
            )
        )
        (move-rest-attrs $c $b)
    )
)
