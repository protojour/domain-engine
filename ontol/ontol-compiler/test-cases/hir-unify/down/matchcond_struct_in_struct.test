(struct ($c)
    (prop! $c R:1:0 $a)
    (prop! $c R:1:1 $b)
)

(matrix (.. @f
    (match-struct ($d)
        (prop! $d R:2:0
            (struct ($e)
                (prop! $e R:3:0 [$a $b])
            )
        )
    )
))

|$c| (block
    (let-prop $a ($c R:1:0))
    (let-prop $b ($c R:1:1))
    (match-struct ($g)
        (let-cond-var $d $g)
        (push-cond-clauses $g
            (root '$d)
        )
        (let-cond-var $e $g)
        (let-cond-var $i $g)
        (push-cond-clauses $g
            (match-prop '$d R:2:0 element-in '$i)
            (member '$i (_ '$e))
        )
        (let-cond-var $h $g)
        (push-cond-clauses $g
            (match-prop '$e R:3:0 element-in '$h)
            (member '$h ($b $a))
        )
    )
)
