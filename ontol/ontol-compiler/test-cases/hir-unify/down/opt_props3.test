(struct ($c)
    (prop? $c R:1:0
        (struct ($d)
            (prop? $d R:1:1 $a)
        )
    )
    (prop? $c R:1:2
        (struct ($e)
            (prop? $e R:1:3 $b)
        )
    )
)

// We should get "O:1:0" if either of "S:1:0" or "S:1:1" are defined(?)
// Then its optional child props should be defined respectively based on free vars
(struct ($f)
    (prop? $f R:2:0
        (struct ($g)
            (prop? $g R:2:1 $a)
            (prop? $g R:2:2 $b)
        )
    )
)

|$c| (block
    (let-prop $d ($c R:1:0))
    (let-prop $e ($c R:1:2))
    (struct ($f)
        (prop?- $f R:2:0
            (struct ($g)
                (catch (@h)
                    (try? @h $d)
                    (let-prop? @h $a ($d R:1:1))
                    (prop?- $g R:2:1 $a)
                )
                (catch (@i)
                    (try? @i $e)
                    (let-prop? @i $b ($e R:1:3))
                    (prop?- $g R:2:2 $b)
                )
            )
        )
        (move-rest-attrs $f $c)
    )
)