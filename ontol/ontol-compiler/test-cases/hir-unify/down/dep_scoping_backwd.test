(struct ($f)
    (prop! $f rel@1:0:0 $a)
    (prop! $f rel@1:0:1 $b)
    (prop! $f rel@1:0:2 $c)
)

(struct ($c)
    (prop! $c rel@2:0:0 (+ $b $c))
    (prop! $c rel@2:0:1 (+ $a $b))
    (prop! $c rel@2:0:2 $a)
)

|$f| (block
    (let-prop $a ($f rel@1:0:0))
    (let-prop $b ($f rel@1:0:1))
    (let-prop $c ($f rel@1:0:2))
    (struct ($c)
        (prop! $c rel@2:0:0 (+ $b $c))
        (prop! $c rel@2:0:1 (+ $a $b))
        (prop! $c rel@2:0:2 $a)
        (move-rest-attrs $c $f)
    )
)
