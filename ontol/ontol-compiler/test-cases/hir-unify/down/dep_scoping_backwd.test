(struct ($f)
    (prop! $f R:1:0 $a)
    (prop! $f R:1:1 $b)
    (prop! $f R:1:2 $c)
)

(struct ($c)
    (prop! $c R:2:0 (+ $b $c))
    (prop! $c R:2:1 (+ $a $b))
    (prop! $c R:2:2 $a)
)

|$f| (block
    (let-prop $a ($f R:1:0))
    (let-prop $b ($f R:1:1))
    (let-prop $c ($f R:1:2))
    (struct ($c)
        (prop! $c R:2:0 (+ $b $c))
        (prop! $c R:2:1 (+ $a $b))
        (prop! $c R:2:2 $a)
        (move-rest-attrs $c $f)
    )
)
