(struct ($c)
    (prop! $c R:1:0 (- $a 10))
    (prop! $c R:1:1
        (struct ($e)
            (prop! $e R:2:0 (- $b 10))
        )
    )
)

(struct ($d)
    (prop! $d R:3:0 (+ $a $b))
    (prop! $d R:3:1 (+ $a 20))
    (prop! $d R:3:2 (+ $b 20))
)

|$c| (block
    (let-prop $f ($c R:1:0))
    (let-prop $e ($c R:1:1))
    (let-prop $g ($e R:2:0))
    (let $a (+ $f 10))
    (let $b (+ $g 10))
    (struct ($d)
        (prop! $d R:3:0 (+ $a $b))
        (prop! $d R:3:1 (+ $a 20))
        (prop! $d R:3:2 (+ $b 20))
        (move-rest-attrs $d $c)
    )
)
