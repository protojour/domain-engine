(struct ($c)
    (prop! $c p@1:0:0 (- $a 10))
    (prop! $c p@1:0:1
        (struct ($e)
            (prop! $e p@2:0:0 (- $b 10))
        )
    )
)

(struct ($d)
    (prop! $d p@3:0:0 (+ $a $b))
    (prop! $d p@3:0:1 (+ $a 20))
    (prop! $d p@3:0:2 (+ $b 20))
)

|$c| (block
    (let-prop $f ($c p@1:0:0))
    (let-prop $e ($c p@1:0:1))
    (let-prop $g ($e p@2:0:0))
    (let $a (+ $f 10))
    (let $b (+ $g 10))
    (struct ($d)
        (prop! $d p@3:0:0 (+ $a $b))
        (prop! $d p@3:0:1 (+ $a 20))
        (prop! $d p@3:0:2 (+ $b 20))
        (move-rest-attrs $d $c)
    )
)
