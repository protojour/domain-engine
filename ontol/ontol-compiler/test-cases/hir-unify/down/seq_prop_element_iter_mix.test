(struct ($e)
    (prop! $e R:1:0 $a)
    (prop! $e R:1:1 (matrix (.. @d $b)))
    (prop! $e R:1:2 $c)
)

(struct ($f)
    (prop! $f R:2:1
        (matrix
            ($a)
            (.. @d $b)
            ($c)
        )
    )
)

|$e| (block
    (let-prop $a ($e R:1:0))
    (let-prop [$d] ($e R:1:1))
    (let-prop $c ($e R:1:2))
    (struct ($f)
        (prop! $f R:2:1
            (make-matrix ($g)
                (insert $g $a)
                (for-each $d ($b)
                    (insert $g $b)
                )
                (insert $g $c)
            )
        )
        (move-rest-attrs $f $e)
    )
)
