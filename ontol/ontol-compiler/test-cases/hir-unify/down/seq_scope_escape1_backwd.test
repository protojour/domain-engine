(struct ($d)
    (prop! $d rel@2:0:0
        (struct ($e)
            (prop! $e rel@3:0:0 #u)
            (prop! $e rel@3:0:1 (matrix (.. @a #u $b)))
        )
    )
)

(struct ($c)
    (prop! $c rel@1:0:0 #u)
    (prop! $c rel@1:0:1 (matrix (.. @a #u $b)))
)

|$d| (block
    (let-prop $e ($d rel@2:0:0))
    (let-prop $_ ($e rel@3:0:0))
    (let-prop [$a $f] ($e rel@3:0:1))
    (struct ($c)
        (prop! $c rel@1:0:0 #u)
        (prop! $c rel@1:0:1
            (make-matrix ($g $h)
                (for-each $a $f ($_ $b)
                    (insert $g #u)
                    (insert $h $b)
                )
            )
        )
        (move-rest-attrs $c $d)
    )
)
