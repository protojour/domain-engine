(struct ($d)
    (prop! $d R:2:0
        (struct ($e)
            (prop! $e R:3:0 #u)
            (prop! $e R:3:1 (set (.. @a #u $b)))
        )
    )
)

(struct ($c)
    (prop! $c R:1:0 #u)
    (prop! $c R:1:1 (set (.. @a #u $b)))
)

|$d| (block
    (let-prop $e ($d R:2:0))
    (let-prop $_ ($e R:3:0))
    (let-prop $a ($e R:3:1))
    (struct ($c)
        (prop! $c R:1:0 #u)
        (prop! $c R:1:1
            (make-seq ($f)
                (for-each $a ($_ $b)
                    (insert $f #u $b)
                )
            )
        )
        (move-rest-attrs $c $d)
    )
)
