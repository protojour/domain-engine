(struct ($c)
    (prop! $c R:1:0
        (#u
            (struct ($d)
                (prop! $d R:2:0 (#u $a))
            )
        )
    )
)

(struct ($d)
    (prop! $d R:3:0 (#u $a))
)

|$c| (block
    (let-prop $_ $d ($c R:1:0))
    (let-prop $_ $a ($d R:2:0))
    (struct ($d)
        (prop! $d R:3:0
            (#u $a)
        )
        (move-rest-attrs $d $c)
    )
)