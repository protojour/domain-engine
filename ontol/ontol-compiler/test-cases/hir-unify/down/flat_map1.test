(struct ($c)
    (prop! $c R:1:0
        (matrix
            (.. @d
                (struct ($e)
                    (prop! $e R:1:2 $a)
                )
            )
        )
    )
    (prop! $c R:1:1 $b)
)

(matrix
    (.. @d
        (struct ($f)
            (prop! $f R:2:0 $a)
            (prop! $f R:2:1 $b)
        )
    )
)

|$c| (block
    (let-prop [$d] ($c R:1:0))
    (let-prop $b ($c R:1:1))
    (make-matrix ($g)
        (for-each $d ($e)
            (let-prop $a ($e R:1:2))
            (insert $g
                (struct ($f)
                    (prop! $f R:2:0 $a)
                    (prop! $f R:2:1 $b)
                )
            )
        )
    )
)