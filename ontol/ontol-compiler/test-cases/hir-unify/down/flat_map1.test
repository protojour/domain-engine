(struct ($c)
    (prop! $c R:1:0
        (#u
            (set
                (.. @d
                    #u
                    (struct ($e)
                        (prop! $e R:1:2
                            (#u $a)
                        )
                    )
                )
            )
        )
    )
    (prop! $c R:1:1 (#u $b))
)

(set
    (.. @d
        #u
        (struct ($f)
            (prop! $f R:2:0
                (#u $a)
            )
            (prop! $f R:2:1
                (#u $b)
            )
        )
    )
)

|$c| (block
    (let-prop $_ $d ($c R:1:0))
    (let-prop $_ $b ($c R:1:1))
    (make-seq ($g)
        (for-each $d ($_ $e)
            (let-prop $_ $a ($e R:1:2))
            (insert $g #u
                (struct ($f)
                    (prop! $f R:2:0
                        (#u $a)
                    )
                    (prop! $f R:2:1
                        (#u $b)
                    )
                )
            )
        )
    )
)