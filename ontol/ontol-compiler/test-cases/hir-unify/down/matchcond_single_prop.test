(struct ($b)
    (prop! $b R:1:0 (#u $a))
)

(set (.. @d
    #u
    (match-struct ($c)
        (prop! $c R:2:0 (#u $a))
    )
))

|$b| (block
    (let-prop $_ $a ($b R:1:0))
    (match-struct ($e)
        (let-cond-var $c $e)
        (push-cond-clauses $e
            (root '$c)
        )
        (let-cond-var $f $e)
        (push-cond-clauses $e
            (match-prop '$c R:2:0 element-in '$f)
            (member '$f (_ $a))
        )
    )
)
