(struct ($b)
    (prop! $b rel@1:0:0 $a)
)

(matrix (.. @d
    (match-struct ($c)
        (prop! $c rel@2:0:0 $a)
    )
))

|$b| (block
    (let-prop $a ($b rel@1:0:0))
    (match-struct ($e)
        (let-cond-var $c $e)
        (push-cond-clauses $e
            (root '$c)
        )
        (let-cond-var $f $e)
        (push-cond-clauses $e
            (match-prop '$c rel@2:0:0 element-in '$f)
            (member '$f (_ $a))
        )
    )
)
