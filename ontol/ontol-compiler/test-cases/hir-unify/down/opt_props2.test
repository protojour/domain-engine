(struct ($b)
    (prop? $b R:1:0
        (struct ($c)
            (prop? $c R:1:1 $a)
        )
    )
)

(struct ($d)
    (prop? $d R:2:0
        (struct ($e)
            (prop? $e R:2:1 $a)
        )
    )
)

|$b| (block
    (let-prop $c ($b R:1:0))
    (struct ($d)
        (prop?- $d R:2:0
            (struct ($e)
                (catch (@f)
                    (try? @f $c)
                    (let-prop? @f $a ($c R:1:1))
                    (prop?- $e R:2:1 $a)
                )
            )
        )
        (move-rest-attrs $d $b)
    )
)