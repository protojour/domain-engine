(struct ($b)
    (prop? $b rel@1:0:0
        (struct ($c)
            (prop? $c rel@1:0:1 $a)
        )
    )
)

(struct ($d)
    (prop? $d rel@2:0:0
        (struct ($e)
            (prop? $e rel@2:0:1 $a)
        )
    )
)

|$b| (block
    (let-prop $c ($b rel@1:0:0))
    (struct ($d)
        (prop?- $d rel@2:0:0
            (struct ($e)
                (catch (@f)
                    (try? @f $c)
                    (let-prop? @f $a ($c rel@1:0:1))
                    (prop?- $e rel@2:0:1 $a)
                )
            )
        )
        (move-rest-attrs $d $b)
    )
)