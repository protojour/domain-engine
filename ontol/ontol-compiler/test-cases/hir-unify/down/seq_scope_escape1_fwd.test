(struct ($c)
    (prop! $c rel@1:0:0 #u)
    (prop! $c rel@1:0:1 (matrix (.. @a #u $b)))
)

(struct ($d)
    (prop! $d rel@2:0:0
        (struct ($e)
            (prop! $e rel@3:0:0 #u)
            (prop! $e rel@3:0:1 (matrix (.. @a #u $b)))
        )
    )
)

|$c| (block
    (let-prop $_ ($c rel@1:0:0))
    (let-prop [$a $f] ($c rel@1:0:1))
    (struct ($d)
        (prop! $d rel@2:0:0
            (struct ($e)
                (prop! $e rel@3:0:0 #u)
                (prop! $e rel@3:0:1
                    (make-matrix ($g $h)
                        (for-each $a $f ($_ $b)
                            (insert $g #u)
                            (insert $h $b)
                        )
                    )
                )
            )
        )
        (move-rest-attrs $d $c)
    )
)
