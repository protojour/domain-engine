(struct ($b)
    (prop $b rel@1:0:0
        (matrix (.. @d $a))
    )
)

(struct ($c)
    (prop $c rel@2:0:1
        (matrix (.. @d $a))
    )
)

|$b| (block
    (let-prop-default [$d] ($b rel@1:0:0) (make-seq))
    (struct ($c)
        (prop- $c rel@2:0:1
            (make-matrix ($e)
                (for-each $d ($a)
                    (insert $e $a)
                )
            )
        )
        (move-rest-attrs $c $b)
    )
)
