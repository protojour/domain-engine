(struct ($c)
    (prop? $c S:1:0
        (#u
            (struct ($d)
                (prop? $d S:2:0
                    (#u $a)
                )
            )
        )
    )
    (prop? $c S:1:1
        (#u
            (struct ($e)
                (prop? $e S:2:1
                    (#u $b)
                )
            )
        )
    )
)

// We should get "O:1:0" if either of "S:1:0" or "S:1:1" are defined(?)
// Then its optional child props should be defined respectively based on free vars
(struct ($f)
    (prop? $f O:1:0
        (#u
            (struct ($g)
                (prop? $g O:2:0
                    (#u $a)
                )
                (prop? $g O:2:1
                    (#u $b)
                )
            )
        )
    )
)

|$c| (block
    (let-prop $_ $d ($c S:1:0))
    (let-prop $_ $e ($c S:1:1))
    (struct ($f)
        (prop? $f O:1:0
            (#u
                (struct ($g)
                    (catch (@h)
                        (try? @h $d)
                        (let-prop? @h $_ $a ($d S:2:0))
                        (prop? $g O:2:0
                            (#u $a)
                        )
                    )
                    (catch (@i)
                        (try? @i $e)
                        (let-prop? @i $_ $b ($e S:2:1))
                        (prop? $g O:2:1
                            (#u $b)
                        )
                    )
                )
            )
        )
        (move-rest-attrs $f $c)
    )
)